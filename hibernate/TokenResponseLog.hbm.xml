<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.fis.cortex.model.des.domain.tknrsplog.basic.BasicTokenResponseLog"
		entity-name="com.fis.cortex.model.des.domain.tknrsplog.TokenResponseLog" table="tknrsplog"
		dynamic-update="true" optimistic-lock="version">
		<cache usage="read-only" />

		<id name="tknReqLogId" column="tknreqlog_id" type="long" />
		<one-to-one name="tokenRequestLog" class="com.fis.cortex.model.des.domain.tknreqlog.TokenRequestLog" constrained="true" />
		<property name="whenCreated" column="whencreated" type="timestamp" not-null="true" />
		<property name="respondedBy" column="respondedby" type="char" not-null="true" />
		<property name="responseId" column="responseId" type="string" length="64" />
		<property name="actionCode" column="actioncode" type="char" not-null="true" />
		<property name="errorRecords" column="errorrecords" type="short" />
		<property name="amRecords" column="amrecords" type="short" />
		<property name="updRecords" column="updrecords" type="short" />
		<property name="cmduRecords" column="cmdurecords" type="short" />
		<property name="verResults" column="verresults" type="string" not-null="true" length="64" />
		<property name="issuerProdId" column="issuerprodid" type="string" length="32" />
		<property name="extCommentId" column="extcommentid" type="string" length="64" />
		<property name="tid" column="tid" type="string" length="15" />
		<property name="tknStatus" column="tknstatus" type="char" />
		<property name="taskId" column="taskid" type="string" length="36" />
		<property name="tokenId" column="token_id" type="long" />
		<property name="crddetId" column="crddet_id" type="long" />
		<property name="errGeneratedBy" column="errgeneratedby" type="char" not-null="true" />
		<property name="errorCode" column="errorCode" type="long" />
		<property name="errorMsg" column="errormsg" type="string" length="256" />
		<property name="recoverable" column="recoverable" type="short" />
		<property name="responseCode" column="responsecode" type="string" not-null="true" length="2" />
		
	</class>

</hibernate-mapping>
