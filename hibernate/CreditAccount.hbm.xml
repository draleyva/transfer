<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
    <class name="com.metavante.cortex.domain.ccs.creditaccount.basic.BasicCreditAccount" table="ccsaccdet">
        <id name="accountId" type="long">
            <column name="acc_id"  default="0" />
            <generator class="select" >
                <param name="key">institution</param>
                <param name="key">currency</param>
                <param name="key">accountNumber</param>
            </generator>           
        </id>
        
		<many-to-one name="institution" class="com.fis.cortex.domain.core.institution.Institution" column="inst_id" access="field" lazy="proxy"/>      
        <property name="cycleCode" type="string" column="cyclecode" />
		<property name="schemeCode" type="string" column="scheme" />
		<property name="profileId" type="string" column="chrgprfid" />
        <property name="accountNumber" type="string" insert="true" update="true" column="accno" />
		<property name="accountClass" type="short" column="classid" not-null="true" />
		<property name="userData1" type="string" column="usrdata1"/>
        <property name="userData2" type="string" column="usrdata2" />
        <property name="percentOfCreditLimitForCash" type="double" column="cashprcnt" precision="8" scale="15" not-null="true" />
        <property name="delinquencyLevel" type="short" column="dlnqlvl" not-null="true" />
        <property name="accountInterestRate" type="double" column="accintrate" precision="8" scale="15" not-null="true" />
        <property name="nextAccountInterestRate" type="double" column="nxtaccintrate" precision="8" scale="15" not-null="true" />
        <property name="minimumPaymentPercentageOverride" type="double" column="percminpay" precision="8" scale="15" not-null="true" />
        
        <many-to-one name="currency" class="com.metavante.cortex.domain.money.Currency"
            column="currcode" access="field" insert="true" update="true" fetch="select" lazy="proxy" />
            
        <component name="definiteInterest" class="com.metavante.cortex.domain.money.PreciseMoney">
            <property name="amount" column="def_interest" not-null="true" access="field"/>
            <many-to-one name="currency" class="com.metavante.cortex.domain.money.Currency"
                        cascade="none" access="field" insert="false" update="false" fetch="select" lazy="proxy">
                <column name="currcode"/>
            </many-to-one>
        </component>
        <component name="provisionalInterest" class="com.metavante.cortex.domain.money.PreciseMoney">
            <property name="amount" column="prov_interest" not-null="true" access="field"/>
            <many-to-one name="currency" class="com.metavante.cortex.domain.money.Currency"
                        cascade="none" access="field" insert="false" update="false" fetch="select"  lazy="proxy">
                <column name="currcode"/>
            </many-to-one>
        </component>
        <component name="previousProvisionalInterest" class="com.metavante.cortex.domain.money.PreciseMoney">
            <property name="amount" column="prev_prov_interest" not-null="true" access="field"/>
            <many-to-one name="currency" class="com.metavante.cortex.domain.money.Currency"
                        cascade="none" access="field" insert="false" update="false" fetch="select" lazy="proxy">
                <column name="currcode"/>
            </many-to-one>
        </component>
        <component name="projectedPenaltyInterest" class="com.metavante.cortex.domain.money.PreciseMoney">
            <property name="amount" column="prjpenint" not-null="true" access="field"/>
            <many-to-one name="currency" class="com.metavante.cortex.domain.money.Currency"
                        cascade="none" access="field" insert="false" update="false" fetch="select" lazy="proxy">
                <column name="currcode"/>
            </many-to-one>
        </component>
        
		<property name="shouldOverLimitPenaltyBeApplied" type="short" column="overlimit_pen_due" not-null="true" />
        <property name="numberOfConsecutiveLatePayments" type="short" column="consec_late_pay" not-null="true" />
        <property name="codeOfNextCycle" type="string" column="nxtcyclecode" />
        <property name="cyclePhase" type="short" column="cycle_phase" not-null="true" />
        <property name="lastCashOperationDate" type="date" column="lstcashmvdate" />
        <property name="markDate" type="date" column="mrkdate" generated="always" update="false" />
        <property name="lastSatisfiedPBD" type="date" column="lstsatdddate" />
        <property name="numberCycles" type="integer" column="numcycles" />
        <property name="accountFlags" type="string" column="ccsaccflags" />
 		<property name="customerCode" type="string" column="custcode" length="16" not-null="true" />
        <property name="branchCode" type="string" column="brncode" length="8" not-null="true" />
        <property name="lastPaymentDate" type="date" column="lstpydate" />
        <property name="corporateAccountType" type="short" column="corp" not-null="true" />
        
        <component name="instalmentBalance" class="com.metavante.cortex.domain.money.Money">
            <property name="amount" column="instbalance" not-null="true" access="field"/>
