    <typedef name="JobType"
        class="com.fis.infrastructure.persistence.hibernate.usertype.GenericEnumUserType">
        <param name="enumClass">com.fis.mkc.domain.model.JobType</param>
        <param name="identifierMethod">getId</param>
    </typedef>
    <typedef name="ServiceType"
        class="com.fis.infrastructure.persistence.hibernate.usertype.GenericEnumUserType">
        <param name="enumClass">com.fis.mkc.domain.model.ServiceType</param>
