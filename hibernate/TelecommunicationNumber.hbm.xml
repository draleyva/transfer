<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.fis.cortex.party.domain.contact.basic.BasicTelecommunicationNumber"
		entity-name="com.fis.cortex.party.domain.contact.TelecommunicationNumber" table="PRT_CM_TELCO_NO">
        <composite-id name="id" class="com.fis.cortex.party.domain.contact.basic.TelecommunicationNumberId">
            <key-many-to-one name="contactMechanism" class="com.fis.cortex.party.domain.contactmechanism.ContactMechanism">
                 <column name="PRT_CM_ID" />
                 <column name="PRT_CMT_ID" />
            </key-many-to-one>
        </composite-id>
        <version name="versionNumber" column="VERNO_CTX" access="field" />
        <property name="areaCode" type="string" column="AREA_CODE" />
        <property name="contactNumber" type="string" column="CONTACT_NO" />
        <property name="countryPrefix" type="string" column="COUNTRY_PREFIX" />
        <property name="extensionNumber" type="string" column="EXTENSION_NO" />
	</class>
</hibernate-mapping>
