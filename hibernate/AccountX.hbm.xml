<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>

	<class name="com.fis.cortex.crdbase.domain.account.basic.BasicAccountX"
		entity-name="com.fis.cortex.crdbase.domain.account.AccountX" table="ACCDET_X" optimistic-lock="version">
		
		<id name="id" type="long">
			<column name="id" />
			<generator class="native">
				<param name="sequence_name">${accdet_x_id.generator.sequence}</param>
			</generator>
		</id>		
		<version name="versionNumber" column="VERNO_CTX" access="field" />
		<many-to-one name="account" class="com.fis.cortex.crdbase.domain.account.Account"
		 column="ACCDET_ID" access="field" />
		 
		<property name="userData1" column="USRDATA1" type="string" length="32" access="field"/>
		<property name="userData2" column="USRDATA2" type="string" length="32" access="field"/>		
		<property name="userData3" column="USRDATA3" type="string" length="32" access="field"/>
		<property name="userData4" column="USRDATA4" type="string" length="32" access="field"/>
		<property name="userData5" column="USRDATA5" type="string" length="32" access="field"/>
		
	</class>
</hibernate-mapping>

