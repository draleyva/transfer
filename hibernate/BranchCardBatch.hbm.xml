<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fis.cortex.domain.custid.basic.BasicBranchCardBatch"
		entity-name="com.fis.cortex.domain.custid.BranchCardBatch"
		dynamic-update="true" optimistic-lock="version" table="BRANCH_CRDBTCH">		
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="native">
				<param name="sequence">BRANCH_CRDBTCH_SEQUENCE</param>
			</generator>
		</id>
		<version name="versionNumber" column="VERNO_CTX" access="field" />		
		<many-to-one name="branch" not-found="ignore" class="com.nomadsoft.cortex.domain.branch.basic.BasicBranch" 
		     entity-name="com.nomadsoft.cortex.domain.branch.Branch">
			<column name="BRANCH_ID" length="10" />			
		</many-to-one>	
		<many-to-one name="cardBatch" not-found="ignore" class="com.nomadsoft.cortex.domain.cardbatch.basic.BasicCardBatch" 
		     entity-name="com.nomadsoft.cortex.domain.cardbatch.CardBatch">
			<column name="CRDBATCH_ID" length="10" />			
		</many-to-one>	
	</class>
</hibernate-mapping>