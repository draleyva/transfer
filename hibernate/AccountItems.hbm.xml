<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>
	<class name="com.fis.cortex.auth.domain.accitems.basic.BasicAccountItems"
		entity-name="com.fis.cortex.auth.domain.accitems.AccountItems" table="ACCITEMS"
		optimistic-lock="version">

		<id name="id" column="ID">
			<generator class="native">
				<param name="sequence_name">${accitems_id.generator.sequence}</param>
			</generator>
		</id>

		<version name="versionNumber" column="VERNO_CTX"></version>

		<property name="messageId" column="MSGID" type="long" length="10"
			not-null="true" access="field" />

		<property name="branchId" column="BRANCH_ID" type="long"
			length="10" not-null="true" access="field" />

		<property name="accountStatus" column="ACCSTAT" type="string"
			not-null="true" access="field" />

		<property name="contraAccountNumber" column="CONACCNO" type="string"
			not-null="true" access="field" />

		<property name="localDate" column="DATELOCAL" type="date"
			not-null="true" access="field" />

		<property name="localTime" column="TIMELOCAL" type="long"
			not-null="true" access="field" />

		<property name="cortexDate" column="CTXDATE" type="date"
			not-null="true" access="field" />

		<property name="transactionType" column="TXNTYPE" type="int"
			not-null="true" access="field" />

		<property name="isReversal" column="REV" not-null="true"
			access="field" />

		<property name="transactionCode" column="TXNCODE" type="int"
			not-null="true" access="field" />

		<property name="cardAcceptorId" column="CRDACPTID" type="string"
			not-null="true" access="field" />

		<property name="amountBill" column="AMTBILL" type="float"
			not-null="true" access="field" />

		<property name="currencyBill" column="CURBILL" type="string"
			not-null="true" access="field" />

		<property name="transactionLogId" column="TLOGID" type="long"
			not-null="true" access="field" />

		<property name="accountBlockedAmount" column="AMTBLK" type="float"
			not-null="true" access="field" />

		<property name="amountAvailable" column="AMTAVL" type="float"
			not-null="true" access="field" />

		<property name="transactionBlockedAmount" column="BLKAMT"
			type="float" not-null="true" access="field" />

		<property name="accountAvailableBalance" column="AVLBAL"
			type="float" not-null="true" access="field" />

		<property name="description" column="DESCRIPTION" type="string"
			not-null="true" access="field" />

		<property name="loadType" column="LOADTYPE" type="int"
			not-null="true" access="field" />

		<property name="loadSource" column="LOADSRC" type="int"
			not-null="true" access="field" />

		<property name="cardId" column="CRDDET_ID" type="long"
			access="field" />

		<many-to-one name="account"
			class="com.fis.cortex.crdbase.domain.account.Account" column="accdet_id"
			access="field" />

		<property name="orgId" column="ORGID" type="long" not-null="true"
			access="field" />

		<property name="feeId" column="FEEID" type="long" not-null="true"
			access="field" />

		<property name="transactionStatus" column="TXNSTATUS" type="int"
			not-null="true" access="field" />

		<property name="amtpad" column="AMTPAD" type="float"
			not-null="true" access="field" />

		<property name="xferid" column="XFERID" type="float"
			not-null="true" access="field" />

		<property name="itemSource" column="ITEMSRC" type="long"
			not-null="true" access="field" />

		<property name="externalCode" column="EXTCODE" type="string"
			not-null="true" access="field" />

		<property name="terminalId" column="TERMPOS_ID" type="long"
			not-null="true" access="field" />

		<property name="externalReferenceId" column="EXTREFID" type="long"
			not-null="true" access="field" />

		<property name="transactionSourceProgId" column="PROGRAM_ID"
			type="long" not-null="true" access="field" />

		<property name="corporateCustomerProgram" column="CORPCUSTPROG_ID"
			type="long" not-null="true" access="field" />

		<property name="transactionAmount" column="AMTTXN" type="float"
			not-null="true" access="field" />

		<property name="transactionCurrency" column="CURTXN" type="string"
			not-null="true" access="field" />

		<property name="billRate" column="RATEBILL" type="float"
			not-null="true" access="field" />

		<property name="reasonCode" column="REASONCODE" type="long"
			not-null="true" access="field" />

		<property name="partialCardNumber" column="PART_PAN" type="string"
			not-null="true" access="field" />

		<property name="amountBillCommission" column="AMTBILLCOM"
			type="float" not-null="true" access="field" />

		<property name="transactionSubType" column="TXNSUBTYPE" type="int"
			not-null="true" access="field" />

		<property name="preAuthBlockedAmount" column="PREAUTH_BLK"
			type="float" not-null="true" access="field" />

		<property name="preAuthUnclearedAmount" column="PREAUTH_UNCLR"
			type="float" not-null="true" access="field" />

		<property name="unclearedCredit" column="UNCLRCREDIT" type="float"
			not-null="true" access="field" />

		<property name="unclearedCreditDifference" column="UNCLRCREDITDIFF"
			type="float" not-null="true" access="field" />
		
		<property name="unclearedLogOnlyCredit" column="UNCLRLOCREDIT" type="float"
			not-null="true" access="field" />
			
		<property name="unclearedLogOnlyCreditDiff" column="UNCLRLOCREDITDIFF" type="float"
			not-null="true" access="field" />

	</class>
</hibernate-mapping>
