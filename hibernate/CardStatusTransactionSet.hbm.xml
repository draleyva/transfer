<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">

<hibernate-mapping>

	<class
		name="com.fis.cortex.crdbase.domain.card.basic.BasicCardStatusTransactionSet"
		entity-name="com.fis.cortex.crdbase.domain.card.CardStatusTransactionSet"
		table="crdstatustransset" dynamic-update="true" optimistic-lock="version">
		<id name="id" type="long" length="10">
			<column name="id" />
			<generator class="native">
				<param name="sequence_name">${crdstatustransset_id.generator.sequence}</param>
			</generator>
		</id>
		<version name="versionNumber" column="verno_ctx" generated="always"
			access="field" />

		<many-to-one name="institution"
			class="com.fis.cortex.domain.core.institution.Institution"
			column="INST_ID" cascade="none" access="field"
			fetch="select" lazy="proxy" not-null="true" />

		<property name="setName" column="SETNAME" not-null="true"
			access="field" />

		<property name="description" column="DESCR" type="string"
			length="30" not-null="true" access="field" />

	</class>

</hibernate-mapping>
