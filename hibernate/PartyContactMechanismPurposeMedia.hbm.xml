<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.fis.cortex.party.domain.media.basic.BasicPartyContactMechanismPurposeMedia"
		entity-name="com.fis.cortex.party.domain.media.PartyContactMechanismPurposeMedia" table="PRT_PCMP_MEDIA" dynamic-update="true">
		<composite-id name="id" class="com.fis.cortex.party.domain.media.basic.PartyContactMechanismPurposeMediaId">
		  <key-property name="mediaId" column="MEDIA_ID" />
		  <key-many-to-one name="partyContactMechanismPurpose" class="com.fis.cortex.party.domain.purpose.PartyContactMechanismPurpose">
		      <column name="PRT_CMPT_ID" />
		      <column name="PRT_PARTY_ID" />
		      <column name="PRT_PRT_ID" />
		      <column name="PRT_CM_ID" />
		      <column name="PRT_CMT_ID" />
		      <column name="PRT_PCM_FROM_DATE" />
		      <column name="PRT_PCMP_FROM_DATE" />
		      <column name="MEDIATYPE_ID" />
		  </key-many-to-one>
		</composite-id>
		<version name="versionNumber" column="VERNO_CTX" access="field" />
    </class>
</hibernate-mapping>
