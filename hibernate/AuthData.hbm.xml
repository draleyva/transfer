<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fis.mkc.domain.model.basic.BasicAuthData"
		entity-name="com.fis.mkc.domain.model.AuthData" dynamic-update="true"
		optimistic-lock="version" table="MKC_AUTH_DATA">
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="native">
				<param name="sequence_name">MKC_AUTH_DATA_SEQUENCE</param>
			</generator>
		</id>
		<version name="versionNumber" column="VERNO_CTX" access="field" />
		<property name="newData" type="java.lang.String">
			<column name="DATA" sql-type="CLOB" />
		</property>
		<property name="originalData" type="java.lang.String">
			<column name="ORIG_DATA" sql-type="CLOB" />
		</property>
		<property name="dataClassName" type="java.lang.String">
			<column name="DATA_TYPE" not-null="true" />
		</property>
		<property name="serviceName" type="java.lang.String">
			<column name="SERVICE" not-null="true" />
		</property>
		<property name="serviceType" type="ServiceType">
			<column name="SERVICE_TYPE" not-null="true" precision="5"
				scale="0" />
		</property>
		<property name="methodName" type="java.lang.String">
			<column name="SERVICE_METHOD" not-null="true" />
		</property>
		<property name="serviceOrder" type="java.lang.Integer">
			<column name="SVC_ORDER" not-null="true" precision="5" scale="0" />
		</property>
		<property name="methodOrder" type="java.lang.Integer">
			<column name="SVC_METHOD_ORDER" not-null="true" precision="5"
				scale="0" />
		</property>
		<property name="paramOrder" type="java.lang.Integer">
			<column name="METHOD_PARAM_ORDER" not-null="true" precision="5"
				scale="0" />
		</property>
		<property name="newDataDisplay" type="java.lang.String">
			<column name="DISPLAY_DATA" sql-type="CLOB" />
		</property>
		<property name="originalDataDisplay" type="java.lang.String">
			<column name="ORIG_DISPLAY_DATA" sql-type="CLOB" />
		</property>
		<property name="entityType" type="java.lang.String">
			<column name="ENTITY_TYPE" />
		</property>
		<property name="entityId" type="java.lang.Long">
			<column name="ENTITY_ID" />
		</property>
		<property name="entityKey" type="java.lang.String">
			<column name="ENTITY_KEY" />
		</property>
		<component name="sourceWebRequest" class="com.fis.mkc.domain.model.basic.BasicSourceWebRequest" >
			<property name="path" type="java.lang.String" column="REQUEST_PATH" length="255"/>
			<property name="params" type="java.lang.String" column="REQUEST_PARAMS" length="4000"/>
			<property name="headers" type="java.lang.String">
				<column name="REQUEST_HEADERS" sql-type="CLOB" />
			</property>
			<property name="data" type="java.lang.String">
				<column name="REQUEST_DATA" sql-type="CLOB" />
			</property>
			<property name="originalData" type="java.lang.String">
				<column name="REQUEST_ORIG_DATA" sql-type="CLOB" />
			</property>
		</component>
		<many-to-one cascade="all" name="jobAuthorization"
			entity-name="com.fis.mkc.domain.model.JobAuthorization" fetch="join">
			<column name="JOB_AUTH_ID" not-null="true" />
		</many-to-one>
	</class>
</hibernate-mapping>