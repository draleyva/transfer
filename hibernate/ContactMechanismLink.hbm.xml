<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.fis.cortex.party.domain.contactmechanism.basic.BasicContactMechanismLink"
		entity-name="com.fis.cortex.party.domain.contactmechanism.ContactMechanismLink" table="PRT_CML">
		<composite-id name="id" class="com.fis.cortex.party.domain.contactmechanism.basic.ContactMechanismLinkId">
		  <key-many-to-one name="contactMechanismIdFrom" column="PRT_CM_ID_FROM" class="com.fis.cortex.party.domain.contactmechanism.ContactMechanism"/>
		  <key-many-to-one name="contactMechanismTypeIdFrom" column="PRT_CMT_ID_FROM" class="com.fis.cortex.party.domain.contactmechanism.ContactMechanism"/>
		  <key-many-to-one name="contactMechanismIdTo" column="PRT_CM_ID_TO" class="com.fis.cortex.party.domain.contactmechanism.ContactMechanism"/>
		  <key-many-to-one name="contactMechanismTypeIdTo" column="PRT_CMT_ID_TO" class="com.fis.cortex.party.domain.contactmechanism.ContactMechanism"/>
		</composite-id>
		<version name="versionNumber" column="VERNO_CTX" access="field" />

	</class>
</hibernate-mapping>
